{
  "version": 1,
  "projects": {
    "myapp": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/myapp",
      "sourceRoot": "apps/myapp/src",
      "prefix": "angular-boss",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/myapp",
            "index": "apps/myapp/src/index.html",
            "main": "apps/myapp/src/main.ts",
            "polyfills": "apps/myapp/src/polyfills.ts",
            "tsConfig": "apps/myapp/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/myapp/src/favicon.ico",
              "apps/myapp/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared/ui/src/assets",
                "output": "./assets"
              }
            ],
            "styles": ["apps/myapp/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/myapp/src/environments/environment.ts",
                  "with": "apps/myapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "myapp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "myapp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "myapp:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/myapp/tsconfig.app.json",
              "apps/myapp/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/myapp/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/myapp/jest.config.js",
            "tsConfig": "apps/myapp/tsconfig.spec.json",
            "setupFile": "apps/myapp/src/test-setup.ts"
          }
        }
      }
    },
    "myapp-e2e": {
      "root": "apps/myapp-e2e",
      "sourceRoot": "apps/myapp-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/myapp-e2e/cypress.json",
            "tsConfig": "apps/myapp-e2e/tsconfig.e2e.json",
            "devServerTarget": "myapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "myapp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/myapp-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/myapp-e2e/**"]
          }
        }
      }
    },
    "shared-data": {
      "root": "libs/shared/data",
      "sourceRoot": "libs/shared/data/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/shared/data/tsconfig.lib.json",
              "libs/shared/data/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/shared/data/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/shared/data/jest.config.js",
            "tsConfig": "libs/shared/data/tsconfig.spec.json"
          }
        }
      }
    },
    "shared-domain": {
      "root": "libs/shared/domain",
      "sourceRoot": "libs/shared/domain/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/shared/domain/tsconfig.lib.json",
              "libs/shared/domain/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/shared/domain/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/shared/domain/jest.config.js",
            "tsConfig": "libs/shared/domain/tsconfig.spec.json"
          }
        }
      }
    },
    "shared-ui": {
      "projectType": "library",
      "root": "libs/shared/ui",
      "sourceRoot": "libs/shared/ui/src",
      "prefix": "ab-ui",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/shared/ui/tsconfig.lib.json",
              "libs/shared/ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/shared/ui/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/shared/ui/jest.config.js",
            "tsConfig": "libs/shared/ui/tsconfig.spec.json",
            "setupFile": "libs/shared/ui/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "regions-store": {
      "projectType": "library",
      "root": "libs/regions-store",
      "sourceRoot": "libs/regions-store/src",
      "prefix": "angular-boss",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/regions-store/tsconfig.lib.json",
              "libs/regions-store/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/regions-store/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/regions-store/jest.config.js",
            "tsConfig": "libs/regions-store/tsconfig.spec.json",
            "setupFile": "libs/regions-store/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "region-quiz": {
      "projectType": "library",
      "root": "libs/region-quiz",
      "sourceRoot": "libs/region-quiz/src",
      "prefix": "angular-boss",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/region-quiz/tsconfig.lib.json",
              "libs/region-quiz/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/region-quiz/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/region-quiz/jest.config.js",
            "tsConfig": "libs/region-quiz/tsconfig.spec.json",
            "setupFile": "libs/region-quiz/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "myapp"
}
